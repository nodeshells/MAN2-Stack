{
  "name": "man2-stack",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm-run-all -s build:server -p start:*",
    "start:client": "ng serve --aot=false --progress=false --base-href /",
    "start:server": "run-p watch:server boot:server",
    "watch:server": "tsc -w -p ./server/tsconfig.server.json",
    "boot:server": "nodemon ./dist/server/src/main.js",
    "build": "run-s build:server build:client",
    "build:client": "ng build --output-path=./dist/server/public --prod --build-optimizer --aot",
    "build:server": "tsc -p ./server/tsconfig.server.json",
    "buildRun": "run-s build:server build:client boot:server",
    "test": "ng test",
    "test:server": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "lint": "ng lint",
    "lint:server": "tslint -p tsconfig.json -c tslint.json",
    "e2e": "ng e2e",
    "format:server": "prettier --write \"src/**/*.ts\"",
    "prestart:serverprod": "rimraf dist && tsc"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.0.3",
    "@angular/common": "^6.0.3",
    "@angular/compiler": "^6.0.3",
    "@angular/core": "^6.0.3",
    "@angular/forms": "^6.0.3",
    "@angular/http": "^6.0.3",
    "@angular/platform-browser": "^6.0.3",
    "@angular/platform-browser-dynamic": "^6.0.3",
    "@angular/router": "^6.0.3",
    "@nestjs/common": "^5.4.0",
    "@nestjs/core": "^5.4.0",
    "core-js": "^2.5.4",
    "npm-run-all": "^4.1.5",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^2.6.2",
    "rxjs": "^6.2.2",
    "typescript": "^2.7.2",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.8",
    "@angular/cli": "~6.0.8",
    "@angular/compiler-cli": "^6.0.3",
    "@angular/language-service": "^6.0.3",
    "@nestjs/testing": "^5.1.0",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.1",
    "@types/node": "^10.7.1",
    "@types/supertest": "^2.0.5",
    "codelyzer": "~4.2.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "jest": "^23.5.0",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "nodemon": "^1.18.3",
    "prettier": "^1.14.2",
    "protractor": "~5.3.0",
    "supertest": "^3.1.0",
    "ts-jest": "^23.1.3",
    "ts-loader": "^4.4.2",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.5.0",
    "tslint": "5.11.0",
    "tslint-config-semistandard": "^7.0.0"
  }
}
